{
  "manifest_version": 3,
  "name": "Chrome Automation API",
  "version": "1.0.0",
  "description": "Production-ready browser automation via local API",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
